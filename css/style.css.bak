*{
	margin:0;
	padding:0;
	color:white;
}

body {
	
	overflow:scroll;
	overflow-x:hidden;
	position: relative;
	width: 100%;
	height:605px;
	font-size:62.5%;
	background-color: #68ced6;

}
.main{
	width:100%;	
	margin:0px auto;
}

.top{
	margin: 0px auto;
	width: 100%;
	height: 100px;
	padding-top:10px;
	padding-left:15px;
}

.m_up{
	margin: 0px auto;
	width: 100%;
	height: 120px;
}
.m_down{
	margin: 0px auto;
	padding:0 10% 10% 10%;		
	font-size:1.6rem;
}

.bottom{
	margin: 0px auto;
	height: 205px;		
}
.b_left{
	float:left;
	padding-left:10%;	
	height: 205px;

}
.b_right{
	float:right;
	padding-right:10%;	
	height: 205px;

}
